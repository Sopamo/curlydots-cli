    "format": "biome format --write ."
  },
  "dependencies": {
    "ink": "^5.0.1",
    "ink-spinner": "^5.0.0",
    "react": "^18.3.1",
    "wink-eng-lite-web-model": "^1.8.1",
    "wink-nlp": "^2.4.0",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/bun": "latest",
    "@types/react": "^18.3.12",
    "ink-testing-library": "^4.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
