{
  "name": "aitranslate",
  "version": "0.1.0",
  "description": "CLI tool to analyze translation files and find missing translations with code context",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "aitranslate": "src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
